{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "/pgvector-db/data/*",
        "watchAssets": true
      },
      {
        "include": "/temp/*",
        "watchAssets": true
      }
    ]
  }
}
